<main class="edit-reservation-page">
    <h2>Editace</h2>
    <form>
        <label for="jmeno">Jméno</label>
        <input type="text" id="jmeno" name="jmeno">
        <label for="prijmeni">Příjmení</label>
        <input type="text" id="prijmeni" name="prijmeni">
        <label for="telefon">Telefon</label>
        <input type="tel" id="telefon" name="telefon">
        <label for="email">Email</label>
        <input type="email" id="email" name="email">
        <label for="cena">Cena</label>
        <input type="text" id="cena" name="cena">
        <label for="sluzba">Služba</label>
        <select id="sluzba" name="sluzba">
            <option value="sluzba1">Služba 1</option>
            <option value="sluzba2">Služba 2</option>
            <option value="sluzba3">Služba 3</option>
        </select>
        <label for="datum">Datum</label>
        <input type="date" id="datum" name="datum">
        <button type="submit">Uložit</button>
    </form>
</main>